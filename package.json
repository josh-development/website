{
  "name": "website",
  "version": "0.0.0",
  "description": "My personal portfolio website",
  "author": "Hezekiah Hendry <hezekiah.hendry@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist node_modules/.cache",
    "lint": "eslint src --ext ts,tsx --fix",
