{
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "style": {
        "noNonNullAssertion": "off"
      },
      "a11y": {
        "useButtonType": "off"
      }
    }
  },
  "formatter": {
    "enabled":true
  },
  "organizeImports": {
    "enabled": true
  },
  "javascript": {
    "formatter": {
      "trailingComma": "none",
      "quoteStyle": "single",
      "semicolons": "always"
    }
  }
}